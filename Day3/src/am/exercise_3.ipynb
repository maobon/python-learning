    "import numpy as np\n",
    "\n",
    "array = np.arange(1, 13).reshape(3, 4)\n",
    "mean_val = np.mean(array)\n",
    "std_val = np.std(array)\n",
    "\n",
    "print(array)\n",
    "print(\"平均值:\", mean_val)\n",
    "print(\"标准差:\", std_val)\n",
    "\n",
    "array[array < mean_val] = -1\n",
    "print(array)\n"
